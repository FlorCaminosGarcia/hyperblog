			{
				"command": "deepseek.acceptCompletion",
				"category": "DeepSeek",
				"title": "%deepseek.command_accept_completion%"
			}
		],
		"menus": {
			"explorer/context": [],
			"editor/context": [
				{
					"submenu": "deepseek/editor/context/menuItems",
					"group": "navigation"
				}
			],
			"view/title": [
				{
					"command": "deepseek.exportMarkdown",
					"when": "view == deepseek.sidebar",
					"group": "navigation"
				},
				{
					"command": "deepseek.clearList",
					"when": "view == deepseek.sidebar",
					"group": "navigation"
				}
			],
			"deepseek/editor/context/menuItems": [
				{
					"command": "deepseek.whyBroken",
					"group": "deepseek-menu-group1@1"
				},
				{
					"command": "deepseek.explain",
					"group": "deepseek-menu-group1@2"
				},
				{
					"command": "deepseek.refactor",
					"group": "deepseek-menu-group1@3"
				},
				{
					"command": "deepseek.addTests",
					"group": "deepseek-menu-group1@4"
				},
				{
					"command": "deepseek.AddPredefinedPrompt",
					"group": "deepseek-menu-group1@5"
				},
				{
					"command": "deepseek.editPredefinedPrompt",
					"group": "deepseek-menu-group1@6"
				},
				{
					"command": "deepseek.choosePredefinedPrompt",
					"group": "deepseek-menu-group1@7"
				},
				{
					"command": "deepseek.customPrompt",
					"group": "deepseek-menu-group1@8"
				},
				{
					"command": "deepseek.unBindAll",
					"group": "deepseek-menu-group2@1"
				},
				{
					"command": "deepseek.switchLanguage",
					"group": "deepseek-menu-group3@1"
				}
			]
		},
		"submenus": [
			{
				"id": "deepseek/editor/context/menuItems",
				"label": "ðŸš€DeepSeek"
			}
		],
		"configuration": {
			"title": "DeepSeek",
			"properties": {
				"deepseek.baseURL": {
					"order": 1,
					"type": "string",
					"default": "",
					"description": "Set up baseURL,Please make sure that your server is properly available"
				},
				"deepseek.model": {
					"order": 2,
					"type": "string",
					"default": "",
					"description": "Set the model of deepseek. Available models: deepseek-chat, deepseek-coder, deepseek-reasoner"
				},
				"deepseek.apikey": {
					"order": 3,
					"type": "string",
					"default": "",
					"password": true,
					"description": "Set the API key of deepseek"
				},
				"deepseek.temperature": {
					"order": 4,
					"type": "string",
					"default": "",
					"description": "Set the temperature of deepseek API,between 0 and 2"
				},
				"deepseek.max_tokens": {
					"order": 5,
					"type": "string",
					"default": "",
					"description": "Set the max_tokens of deepseek API. Max value is 8192 when using beta endpoint"
				},
				"deepseek.roleDefinition": {
					"order": 6,
					"type": "string",
					"default": "",
					"description": "Set the system role definition of deepseek API. If not set, will use default role definition"
				},
				"deepseek.lang": {
					"order": 7,
					"type": "string",
					"default": "",
					"description": "Set the language of deepseek."
				},
				"deepseek.top_p": {
					"order": 8,
					"type": "string",
					"default": "",
					"description": "Set the top_p of deepseek API, between 0 and 1"
				},
				"deepseek.top_k": {
					"order": 9,
					"type": "string",
					"default": "",
					"description": "Set the top_k of deepseek API"
				},
				"deepseek.frequency_penalty": {
					"order": 10,
					"type": "string",
					"default": "",
					"description": "The frequency_penalty parameter is used to adjust the frequency of tokens in the generated text"
				},
				"deepseek.enableCompletion": {
					"order": 11,
					"type": "boolean",
					"default": false,
					"description": "Enable AI code completion feature"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"icon": "node icon.js",
		"pack": "vsce package"
	},
